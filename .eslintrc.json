{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies": "off",
        "semi":"off",
        "comma-dangle":"off",
        "space-before-function-paren":"off"
    }
}
